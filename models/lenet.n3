use Conv2d
use Linear

use ReLU
use Softmax
use Transform

[LeNet]

  * N: number of channels = 10

  [Conv2d]
    * kernel size = 5
    * stride = 2

  [Softmax]
    * axis = -1

  #0 Input                =  1, 28, 28
  #1 Conv2d (#0) + ReLU   = 32, 14, 14
  #2 Conv2d      + ReLU   = 64,  7,  7
  #3 Transform            = 64*  7*  7
  #4 Linear + Softmax     =  N
